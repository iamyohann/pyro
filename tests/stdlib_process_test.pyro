import std.process

let output = process.exec("echo", ["hello"])
if output["code"] != 0:
    print("FAILED: exec code")
    print(output["code"])
else:
    if output["stdout"].contains("hello"):
        print("PASSED: exec stdout")
    else:
        print("FAILED: exec stdout")
        print(output["stdout"])

